{
  "baseUrl": "<%= request.base_url %>",
  "description": "<%= @config.description %>",
  "enableLicensing": <%= @config.enable_licensing %>,
  "key": "<%= @config.key %>",
  "name": "<%= @config.name %>",
  "vendor": {
    "name": "<%= @config.vendor_name %>",
    "url": "<%= @config.vendor_url %>"
  },
  "authentication": {
    "type": "jwt"
  },
  <%- if @config.api_version %>
  "apiVersion": <%= @config.api_version %>,
  <%- end %>
  "lifecycle": {
    "disabled": "<%= atlassian_connect.disabled_lifecycle_index_path %>",
    "enabled": "<%= atlassian_connect.enabled_lifecycle_index_path %>",
    "installed": "<%= atlassian_connect.installed_lifecycle_index_path %>",
    "uninstalled": "<%= atlassian_connect.uninstalled_lifecycle_index_path %>"
  },
  "links": {
    "postInstallUrl": "<%= @config.post_install_url %>",
    "postUpdateUrl": "<%= @config.post_update_url %>"
  },
  "modules": <%= raw @config.modules.to_json %>,
  "scopes": <%= raw @config.scopes.to_json %>
}
